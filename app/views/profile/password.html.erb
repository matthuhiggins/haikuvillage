<%= render :layout => 'layout' do %>
  <%= form_tag do %>
    <table>
      <tr>
        <td><%= label_tag :password, 'New password:' %></td>
        <td><%= password_field_tag :password, '', autocomplete: 'off' %></td>
      </tr>
      <tr>
        <td></td>
        <td><%= submit_tag 'Save' %></td>
      </tr>
    </table>
  <% end %>
<% end %>

<script>
$(function() {
  $('#password').focus();
});
</script>